    <script>
        // Device Type Detection
        function detectDeviceType() {
            const width = window.innerWidth;
            if (width < 768) {
                document.body.setAttribute('data-device', 'mobile');
            } else if (width < 1200) {
                document.body.setAttribute('data-device', 'tablet');
            } else {
                document.body.setAttribute('data-device', 'desktop');
            }
        }
        
        // Initialize after DOM is loaded
        document.addEventListener('DOMContentLoaded', function() {
            detectDeviceType();
            initSidebar();
            initAnimations();
            createParticles();
            initAdvancedFeatures();
        });
        
        // Detect device on resize
        window.addEventListener('resize', function() {
            detectDeviceType();
        });
        
        // Initialize sidebar interactions
        function initSidebar() {
            const navItems = document.querySelectorAll('.nav-item');
            
            navItems.forEach(item => {
                item.addEventListener('click', function(e) {
                    e.preventDefault();
                    
                    // Handle sections
                    if (this.hasAttribute('data-section')) {
                        const section = this.getAttribute('data-section');
                        handleSectionNavigation(section, this);
                    }
                    
                    // Handle AI modes
                    if (this.hasAttribute('data-ai-mode')) {
                        const aiMode = this.getAttribute('data-ai-mode');
                        activateAIMode(aiMode, this);
                    }
                    
                    // Handle advanced features
                    if (this.hasAttribute('data-feature')) {
                        const feature = this.getAttribute('data-feature');
                        activateAdvancedFeature(feature, this);
                    }
                });
            });
            
            // Close chat space button
            document.getElementById('close-chat-space').addEventListener('click', function() {
                document.getElementById('chat-space').classList.remove('active');
                // Reset active state in sidebar
                document.querySelector('[data-section="dashboard"]').classList.add('active');
                document.querySelector('[data-section="chat"]').classList.remove('active');
            });
            
            // Chat mode options
            const chatModeOptions = document.querySelectorAll('.chat-mode-option');
            chatModeOptions.forEach(option => {
                option.addEventListener('click', function() {
                    chatModeOptions.forEach(opt => opt.classList.remove('active'));
                    this.classList.add('active');
                    
                    const mode = this.getAttribute('data-mode');
                    changeChatMode(mode);
                });
            });
            
            // Send button for chat
            const sendButtons = document.querySelectorAll('.chat-send-btn');
            sendButtons.forEach(button => {
                button.addEventListener('click', function() {
                    const inputContainer = this.closest('.chat-input-container');
                    const input = inputContainer.querySelector('.chat-input');
                    sendChatMessage(input.value, this.closest('.chat-container'));
                    input.value = '';
                });
            });
            
            // Send on Enter key
            const chatInputs = document.querySelectorAll('.chat-input');
            chatInputs.forEach(input => {
                input.addEventListener('keydown', function(e) {
                    if (e.key === 'Enter') {
                        const container = this.closest('.chat-container');
                        sendChatMessage(this.value, container);
                        this.value = '';
                    }
                });
            });
        }
        
        // Handle section navigation
        function handleSectionNavigation(section, navItem) {
            // Remove active class from all items
            document.querySelectorAll('.nav-item').forEach(item => {
                item.classList.remove('active');
            });
            
            // Add active class to current item
            navItem.classList.add('active');
            
            // Handle specific sections
            if (section === 'chat') {
                document.getElementById('chat-space').classList.add('active');
            } else if (section === 'dashboard') {
                document.getElementById('chat-space').classList.remove('active');
            } else if (section === 'all-ai-modes') {
                alert('All AI Modes view will be implemented in the next update.');
            } else if (section === 'settings') {
                alert('Settings page will be implemented in the next update.');
            }
        }
        
        // Activate AI mode
        function activateAIMode(mode, navItem) {
            // Remove active class from all items
            document.querySelectorAll('.nav-item').forEach(item => {
                item.classList.remove('active');
            });
            
            // Add active class to current item
            navItem.classList.add('active');
            
            // Open chat with specific mode
            document.getElementById('chat-space').classList.add('active');
            
            // Update chat header
            let modeTitle = 'JAAT-AI Assistant';
            if (mode === 'content-writer') modeTitle = 'Content Writer';
            else if (mode === 'code-assistant') modeTitle = 'Code Assistant';
            else if (mode === 'creative') modeTitle = 'Creative Assistant';
            else if (mode === 'data-analysis') modeTitle = 'Data Analysis';
            else if (mode === 'image-generation') modeTitle = 'Image Generation';
            
            // Update chat mode title and clear messages except first welcome
            const chatTitle = document.querySelector('.chat-space .chat-title');
            chatTitle.textContent = modeTitle;
            
            // Reset messages
            const chatMessages = document.querySelector('.chat-space .chat-messages');
            const firstMessage = chatMessages.querySelector('.message');
            chatMessages.innerHTML = '';
            
            // Add appropriate welcome message based on mode
            let welcomeMessage = '';
            if (mode === 'content-writer') {
                welcomeMessage = "Welcome to the Content Writer mode. I'll help you create engaging, SEO-optimized content for your blog, website, or social media. What kind of content would you like to create today?";
            } else if (mode === 'code-assistant') {
                welcomeMessage = "Welcome to the Code Assistant mode. I can help you write, debug, and explain code across various programming languages. What coding task are you working on?";
            } else if (mode === 'creative') {
                welcomeMessage = "Welcome to the Creative Assistant mode. I'll help you with creative writing, storytelling, scripts, poems, and other creative content. What would you like to create?";
            } else if (mode === 'data-analysis') {
                welcomeMessage = "Welcome to the Data Analysis mode. I can help you interpret data, create visualization suggestions, and extract insights from your information. What data would you like to analyze?";
            } else if (mode === 'image-generation') {
                welcomeMessage = "Welcome to the Image Generation mode. I can create image prompts and help you generate visual content. What kind of image would you like to create?";
            }
            
            // Add the welcome message
            addBotMessage(welcomeMessage, chatMessages);
        }
        
        // Activate advanced feature
        function activateAdvancedFeature(feature, navItem) {
            // Switch active class
            document.querySelectorAll('.nav-item').forEach(item => {
                item.classList.remove('active');
            });
            navItem.classList.add('active');
            
            // Show a notification for now
            const featureNames = {
                'quantum-sync': 'Quantum Synchronization',
                'neural-pathway': 'Neural Pathway Integration',
                'holographic-data': 'Holographic Data Tessellation'
            };
            
            const featureName = featureNames[feature] || feature;
            alert(`${featureName} feature will be activated in a future update.`);
        }
        
        // Change chat mode
        function changeChatMode(mode) {
            const chatTitle = document.querySelector('.chat-space .chat-title');
            let title = 'JAAT-AI';
            
            if (mode === 'assistant') title += ' Assistant';
            else if (mode === 'creative') title += ' Creative';
            else if (mode === 'code') title += ' Code Expert';
            
            chatTitle.textContent = title;
        }
        
        // Send chat message
        function sendChatMessage(message, container) {
            if (!message.trim()) return;
            
            // Get the messages container
            const messagesContainer = container.querySelector('.chat-messages');
            
            // Add user message
            const userMessageHtml = `
                <div class="message user">
                    <div class="message-avatar">
                        <i class="fas fa-user"></i>
                    </div>
                    <div class="message-content">
                        <p>${message}</p>
                        <div class="message-time">Just now</div>
                    </div>
                </div>
            `;
            messagesContainer.innerHTML += userMessageHtml;
            
            // Simulate typing indicator
            const typingIndicator = `
                <div class="message typing-indicator">
                    <div class="message-avatar">
                        <i class="fas fa-robot"></i>
                    </div>
                    <div class="message-content">
                        <div class="typing-dots">
                            <span></span>
                            <span></span>
                            <span></span>
                        </div>
                    </div>
                </div>
            `;
            messagesContainer.innerHTML += typingIndicator;
            
            // Scroll to bottom
            messagesContainer.scrollTop = messagesContainer.scrollHeight;
            
            // Simulate AI response after a delay
            setTimeout(() => {
                // Remove typing indicator
                const indicator = messagesContainer.querySelector('.typing-indicator');
                if (indicator) {
                    messagesContainer.removeChild(indicator);
                }
                
                // Generate response based on input
                let response = "I'm processing your request and will assist you momentarily.";
                
                if (message.toLowerCase().includes('hello') || message.toLowerCase().includes('hi')) {
                    response = "Hello! How can I help you today?";
                } else if (message.toLowerCase().includes('feature') || message.toLowerCase().includes('do')) {
                    response = "JAAT-AI offers numerous advanced features including multimodal processing (text, images, audio), neural pathway integration, holographic data visualization, quantum synchronization, and much more. Would you like me to explain any specific feature in detail?";
                } else if (message.toLowerCase().includes('help')) {
                    response = "I'm here to help with various tasks. You can ask me to generate content, answer questions, analyze data, assist with code, generate creative ideas, and much more. Just let me know what you need!";
                }
                
                // Add bot message
                addBotMessage(response, messagesContainer);
            }, 1500);
        }
        
        // Add bot message
        function addBotMessage(message, messagesContainer) {
            const botMessageHtml = `
                <div class="message">
                    <div class="message-avatar">
                        <i class="fas fa-robot"></i>
                    </div>
                    <div class="message-content">
                        <p>${message}</p>
                        <div class="message-time">Just now</div>
                    </div>
                </div>
            `;
            messagesContainer.innerHTML += botMessageHtml;
            
            // Scroll to bottom
            messagesContainer.scrollTop = messagesContainer.scrollHeight;
        }
        
        // Initialize animations
        function initAnimations() {
            // Add animated class to dashboard cards with a delay
            const cards = document.querySelectorAll('.dashboard-card');
            cards.forEach((card, index) => {
                setTimeout(() => {
                    card.classList.add('animated');
                }, 100 * index);
            });
            
            // Add animated class to AI mode cards with a delay
            const modeCards = document.querySelectorAll('.ai-mode-card');
            modeCards.forEach((card, index) => {
                setTimeout(() => {
                    card.classList.add('animated');
                }, 100 * index + 300);
            });
        }
        
        // Create particles for chat space background
        function createParticles() {
            const chatSpaceBg = document.querySelector('.chat-space-bg');
            
            // Create particles
            for (let i = 0; i < 50; i++) {
                const particle = document.createElement('div');
                particle.classList.add('particle');
                
                // Random position
                const x = Math.random() * 100;
                const y = Math.random() * 100;
                
                // Random size
                const size = Math.random() * 5 + 2;
                
                // Set styles
                particle.style.left = `${x}%`;
                particle.style.top = `${y}%`;
                particle.style.width = `${size}px`;
                particle.style.height = `${size}px`;
                
                // Random animation
                const duration = Math.random() * 20 + 10;
                const delay = Math.random() * 10;
                
                particle.style.animation = `float ${duration}s ease-in-out ${delay}s infinite`;
                
                chatSpaceBg.appendChild(particle);
            }
        }
        
        // Initialize advanced features
        function initAdvancedFeatures() {
            // This function will initialize the 20 advanced features
            // For now, it's just a placeholder
            console.log('Initializing advanced features...');
            
            // In a real implementation, this would load the advanced features
            const advancedFeatures = [
                'Quantum Synchronization',
                'Neural Pathway Integration',
                'Adaptive Flux Calibration',
                'Holographic Data Tessellation',
                'Biometric Resonance Mapping',
                'Temporal Compression Matrix',
                'Distributed Cognition Framework',
                'Polymorphic Interface Engine',
                'Ambient Intelligence Orchestration',
                'Hyperdimensional Indexing',
                'Predictive Analytics Membrane',
                'Autonomous Decision Matrix',
                'Semantic Vector Acceleration',
                'Quantum Entanglement Protocol',
                'Adaptive Learning Lattice',
                'Multimodal Inference Engine',
                'Self-Healing Network Fabric',
                'Contextual Awareness Pipeline',
                'Deep Pattern Recognition Core',
                'Evolutionary Algorithm Nexus'
            ];
            
            // Register them with the system
            if (window.JAAT) {
                advancedFeatures.forEach(feature => {
                    const featureId = feature.toLowerCase().replace(/\s+/g, '-');
                    console.log(`Registered advanced feature: ${feature}`);
                });
            }
        }
    </script>
    
