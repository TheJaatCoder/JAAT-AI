                <circle cx="40" cy="45" r="5" fill="white"></circle>
                <circle cx="60" cy="45" r="5" fill="white"></circle>
                <path d="M40,60 Q50,70 60,60" fill="none" stroke="white" stroke-width="2" stroke-linecap="round"></path>
            </svg>
            <div class="logo-text">JAAT-AI</div>
        </div>
        
        <nav class="nav-menu">
            <div class="menu-category">Main</div>
            <a href="#" class="nav-item active" data-section="dashboard">
                <div class="nav-icon"><i class="fas fa-house"></i></div>
                <div class="nav-text">Dashboard</div>
            </a>
            <a href="#" class="nav-item" data-section="chat">
                <div class="nav-icon"><i class="fas fa-comments"></i></div>
